volatile int x = 0, z = 0;

void * thr1(void * arg)
{
while (1) {
	z = 3;
	while (x != 0){;}
}
}

void * thr2(void * arg)
{
while (1) {
	x = 0;
	z = 2;
}
}

void * thr3(void * arg)
{
while (1) {
	x = 0;
}
}

void * thr4(void * arg)
{
while (1) {
	z = 3;
	while (x != 0){;}
}
}


void * thr5(void * arg)
{
while (1) {
	z = 3;
	while (x != 0){;}
}
}


int main()
{
  __CPROVER_ASYNC_0: thr1(0);
  __CPROVER_ASYNC_1: thr2(0);
  __CPROVER_ASYNC_2: thr3(0);

  return 0;
}

