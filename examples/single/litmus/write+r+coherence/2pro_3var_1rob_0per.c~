volatile int x = 0, y = 0, z = 0;

void * thr1(void * arg)
{
while (1) {
   	x = 1;
	z = 3;
	while (x != 1) {;}
}
}

void * thr2(void * arg)
{
while (1) {
	x = 1;
	y = 2;
}
}

void * thr3(void * arg)
{
while (1) {
	y = 3;
	z = 1;
}
}

int main()
{
  __CPROVER_ASYNC_0: thr1(0);
  __CPROVER_ASYNC_1: thr2(0);
  __CPROVER_ASYNC_2: thr3(0);

  return 0;
}

